{
	"name": "sketch2code",
	"version": "0.0.1",
	"description": "From Sketch symbol to pseudo-HTML to proper code",
	"main": "src/server/index.js",
	"author": "Alberto Maghini",
	"license": "MIT",
	"private": true,
	"devDependencies": {
		"@types/chai": "^4.1.3",
		"@types/debug": "^0.0.30",
		"@types/dotenv": "^4.0.3",
		"@types/express": "^4.17.0",
		"@types/glob": "^5.0.35",
		"@types/jszip": "^3.1.6",
		"@types/mocha": "^5.2.1",
		"@types/multer": "^1.3.6",
		"@types/node": "^10.3.0",
		"chai": "^4.1.2",
		"mocha": "^5.2.0",
		"nyc": "^12.0.1",
		"source-map-support": "^0.5.6",
		"ts-node": "^6.0.5",
		"tslint": "^5.10.0",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^2.9.1"
	},
	"scripts": {
		"lint": "tslint -c tslint.json 'src/**/*.ts'",
		"pretest": "yarn run lint && tsc",
		"test": "nyc mocha",
		"prestart": "yarn run pretest",
		"start-cli": "node src/index.js",
		"start": "node src/server/index.js"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"exclude": [
			"**/*.d.ts",
			"test",
			"coverage",
			"src/wrappers"
		],
		"reporter": [
			"html",
			"text-summary"
		],
		"all": true,
		"cache": true
	},
	"dependencies": {
		"debug": "^3.1.0",
		"dotenv": "^6.0.0",
		"express": "^4.16.3",
		"glob": "^7.1.4",
		"jszip": "^3.1.5",
		"multer": "^1.3.0",
		"pug": "^2.0.3"
	}
}
