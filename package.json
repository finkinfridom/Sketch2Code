{
	"name": "sketch2code",
	"version": "0.0.1",
	"description": "From Sketch symbol to pseudo-HTML to proper code",
	"main": "src/index.js",
	"author": "Alberto Maghini",
	"license": "MIT",
	"private": true,
	"devDependencies": {
		"@types/chai": "^4.1.3",
		"@types/jszip": "^3.1.4",
		"@types/mocha": "^5.2.1",
		"@types/node": "^10.3.0",
		"chai": "^4.1.2",
		"mocha": "^5.2.0",
		"nyc": "^12.0.1",
		"source-map-support": "^0.5.6",
		"ts-node": "^6.0.5",
		"tslint": "^5.10.0",
		"tslint-config-prettier": "^1.13.0",
		"typescript": "^2.9.1"
	},
	"scripts": {
		"lint": "tslint -c tslint.json 'src/**/*.ts'",
		"pretest": "yarn run lint && tsc",
		"test": "nyc mocha"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"exclude": [
			"**/*.d.ts",
			"test",
			"coverage",
			"src/wrappers"
		],
		"reporter": [
			"html",
			"text-summary"
		],
		"all": true,
		"cache": true
	},
	"dependencies": {
		"axios": "^0.18.0",
		"jszip": "^3.1.5"
	}
}
